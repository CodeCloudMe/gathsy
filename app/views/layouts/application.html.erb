<!DOCTYPE html>
<html>
<head>
  <title>cloudbnb</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= stylesheet_link_tag 'gmaps4rails' %>
</head>
<body>

  <header class="group" id="site-header">
    <%= render "header/flash_errors" %>
    <%= render "header/flash_notices" %>
    <%= render "header/logo_search_bar" %>
    <%= render "header/login_logout_banner" %>
  </header>

  <main>
    <%= yield %>
  </main>

  <footer>
    <%= yield :scripts %>
  </footer>
</body>
</html>
